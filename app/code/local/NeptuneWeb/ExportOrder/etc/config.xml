<?xml version="1.0"?>
<config>
    <modules>
        <NeptuneWeb_ExportOrder>
            <version>0.1</version>
        </NeptuneWeb_ExportOrder>
    </modules>
    <global>
        <models>
            <nwExportOrder>
                <class>NeptuneWeb_ExportOrder_Model</class>
            </nwExportOrder>
        </models>
        <events>
            <sales_order_payment_place_end>
                <observers>
                    <nwExportOrder>
                        <type>singleton</type>
                        <class>nwExportOrder/observer</class>
                        <method>sales_order_payment_place_end</method>
                    </nwExportOrder>
                </observers>
            </sales_order_payment_place_end>
        </events>
    </global>
</config>
